{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "outDir": "cypress",
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2020", "dom"],
    "types": ["node", "cypress"],
    "typeRoots": ["../../../node_modules"],
    "sourceMap": false
  },
  "include": [
    "**/*.ts",
    "**/*.js",
    "../cypress.config.ts",
    "../**/*.cy.ts",
    "../**/*.cy.tsx",
    "../**/*.cy.js",
    "../**/*.cy.jsx",
    "../**/*.d.ts"
  ]
}
